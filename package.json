    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-unicorn": "^50.0.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.1.1"
  }
